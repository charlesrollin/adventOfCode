{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ESNext"],
    "outDir": "dist",
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/"],
      "typings/*": ["./src/typings/*"],
      "Inputs/*": ["./src/__inputs__/*"],
      "Solvers/*": ["./src/solvers/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["src/test/**/*.spec.ts"]
}
